{
  "name": "redish",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/redish/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/redish"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "redish:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "redish:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "redish:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "redish:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "redish:build:development"
        },
        "production": {
          "buildTarget": "redish:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/redish"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/redish/**/*.{ts,svelte,spec.ts}"]
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "redish-check",
        "cwd": "apps/redish"
      }
    }
  },
  "tags": []
}
